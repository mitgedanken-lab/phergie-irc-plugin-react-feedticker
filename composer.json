{
    "name": "phergie/phergie-irc-plugin-react-feedticker",
    "description": "Phergie plugin for syndicating data from feed items to channels or users",
    "keywords": [ "phergie", "irc", "bot", "plugin" ],
    "license": "BSD-2-Clause",
    "support": {
        "email": "team@phergie.org",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/phergie/phergie-irc-plugin-react-feedticker/issues",
        "source": "https://github.com/phergie/phergie-irc-plugin-react-feedticker"
    },
    "minimum-stability": "dev",
    "require": {
        "phergie/phergie-irc-bot-react": "dev-master",
        "wyrihaximus/phergie-http": "dev-master",
        "zendframework/zend-feed": "2.3.*",
        "zendframework/zend-servicemanager": "2.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "phake/phake": "2.0.0-beta2"
    },
    "autoload": {
        "psr-4": {
            "Phergie\\Irc\\Plugin\\React\\FeedTicker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phergie\\Irc\\Tests\\Plugin\\React\\FeedTicker\\": "tests"
        }
    }
}
